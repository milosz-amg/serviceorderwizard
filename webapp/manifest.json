{
  "_version": "1.76.0",
  "sap.app": {
    "id": "com.mr.serviceorderwizard",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:basic",
      "version": "1.18.6",
      "toolsId": "f0a192fc-b2ff-44be-8dc3-e01bde7ca180"
    },
    "dataSources": {
      "mainService": {
        "uri": "http://hana.sygeon.com:8000/sap/opu/odata/sap/ZMR_ORDER_SRV_SRV/",
        "type": "OData",
        "settings": {
          "odataVersion": "2.0"
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": false,
    "dependencies": {
      "minUI5Version": "1.139.0",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {},
        "sap.ui.layout": {},
        "sap.ui.unified": {},
        "sap.ui.comp": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "com.mr.serviceorderwizard.i18n.i18n",
          "supportedLocales": ["", "pl", "en"],
         "fallbackLocale": "pl"
        }
      },
      "": {
        "type": "sap.ui.model.odata.v2.ODataModel",
        "dataSource": "mainService",
        "settings": {
          "defaultOperationMode": "Server",
          "defaultBindingMode": "OneWay",
          "defaultCountMode": "Request"
        }
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "controlAggregation": "pages",
        "controlId": "app",
        "transition": "slide",
        "type": "View",
        "viewType": "XML",
        "path": "com.mr.serviceorderwizard.view",
        "async": true,
        "viewPath": "com.mr.serviceorderwizard.view"
      },
      "routes": [
        {
          "name": "RouteHome",
          "pattern": ":?query:",
          "target": [
            "TargetHome"
          ]
        }
          ,
          {
            "name": "RouteCreateOrder",
            "pattern": "createorder",
            "target": ["TargetCreateOrder"]
          },
          {
  "name": "RouteOrders",
  "pattern": "orders",
  "target": ["TargetOrders"]
}
      ],
      "targets": {
        "TargetHome": {
          "id": "Home",
          "name": "Home"
        }
          ,
          "TargetCreateOrder": {
            "id": "CreateOrder",
            "name": "CreateOrder"
          },
          "TargetOrders": {
            "id": "Orders",
            "name": "Orders"
          }
      }
    },
    "rootView": {
      "viewName": "com.mr.serviceorderwizard.view.App",
      "type": "XML",
      "id": "App",
      "async": true
    }
  }
}
